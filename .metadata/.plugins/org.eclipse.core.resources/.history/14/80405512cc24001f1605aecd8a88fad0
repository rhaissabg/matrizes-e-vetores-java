package vetores.e.matrizes;

import java.util.Arrays;
import java.util.Scanner;

public class PosicaoNoVetor {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int numero, indice = 0, vetorNumeros[] = {2, 5, 1, 3, 4, 9, 7, 8, 10, 6};
		
		System.out.print("Digite o n√∫mero que deseja encontrar: ");
		numero = sc.nextInt();
		
		Arrays.sort(vetorNumeros);
		
		for (int i = 0; i < vetorNumeros.length; i++) {
			System.out.println(vetorNumeros[i]);
		}
		
		
		sc.close();

	}

}
